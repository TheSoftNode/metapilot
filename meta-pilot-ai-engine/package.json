{
  "name": "@metapilot/ai-engine",
  "version": "1.0.0",
  "description": "Advanced AI engine for Web3 automation and governance decisions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && npm run build:esm",
    "build:esm": "tsc --module esnext --outDir dist/esm",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepare": "npm run build",
    "docs": "typedoc src --out docs"
  },
  "keywords": [
    "ai",
    "web3",
    "dao",
    "governance",
    "automation",
    "nlp",
    "blockchain",
    "decision-engine"
  ],
  "author": "MetaPilot Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/metapilot/ai-engine"
  },
  "dependencies": {
    "compromise": "^14.11.0",
    "natural": "^6.12.0",
    "node-cache": "^5.1.2",
    "openai": "^4.24.7",
    "sentiment": "^5.0.2",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/natural": "^5.1.3",
    "@types/node": "^20.9.0",
    "@types/sentiment": "^5.0.4",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.25.4",
    "typescript": "^5.2.2"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0"
  }
}
